{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "esnext",
    "types": ["vitest/globals"],
    "noImplicitAny": true,
    "outDir": "./build",
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "exclude": ["node_modules/**/*"]
}
